{
    "name": "tanthammar/recurring",
    "description": "Laravel Eloquent RRULE tools",
    "keywords": [
        "tanthammar",
        "laravel",
        "recurring",
        "rrule"
    ],
    "homepage": "https://github.com/tanthammar/recurring",
    "license": "MIT",
    "authors": [
        {
            "name": "Tina Hammar",
            "email": "tinahammar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0"
        "simshaun/recurr": "^5.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "TantHammar\\Recurring\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TantHammar\\Recurring\\RecurringServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
